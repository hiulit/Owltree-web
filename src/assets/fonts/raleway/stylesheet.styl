@font-face {
    font-family: 'Raleway';
    src: url('../assets/fonts/raleway/hinted-Raleway-Regular.woff2') format('woff2'),
        url('../assets/fonts/raleway/hinted-Raleway-Regular.woff') format('woff'),
        url('../assets/fonts/raleway/hinted-Raleway-Regular.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'Raleway';
    src: url('../assets/fonts/raleway/hinted-Raleway-Bold.woff2') format('woff2'),
        url('../assets/fonts/raleway/hinted-Raleway-Bold.woff') format('woff'),
        url('../assets/fonts/raleway/hinted-Raleway-Bold.ttf') format('truetype');
    font-weight: bold;
    font-style: normal;
}

@font-face {
    font-family: 'Raleway';
    src: url('../assets/fonts/raleway/hinted-Raleway-Light.woff2') format('woff2'),
        url('../assets/fonts/raleway/hinted-Raleway-Light.woff') format('woff'),
        url('../assets/fonts/raleway/hinted-Raleway-Light.ttf') format('truetype');
    font-weight: 300;
    font-style: normal;
}

